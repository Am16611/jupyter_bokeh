{
  "name": "@bokeh/jupyter_bokeh",
  "version": "3.0.0",
  "description": "A Jupyter extension for rendering Bokeh content.",
  "author": "Bokeh Team <info@bokeh.org>",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "bokeh",
    "bokehjs"
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc && webpack -p",
    "clean": "rimraf dist lib",
    "lint": "eslint -c eslint.json src/**/*.ts",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w --listEmittedFiles"
  },
  "main": "lib/index.js",
  "files": [
    "{dist,lib}/**/*.{js,js.map,d.ts}",
    "style/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bokeh/jupyter_bokeh.git"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3.0.0",
    "@jupyterlab/application": "^3.0.6",
    "@jupyterlab/apputils": "^3.0.5",
    "@jupyterlab/docregistry": "^3.0.6",
    "@jupyterlab/notebook": "^3.0.6",
    "@jupyterlab/rendermime-interfaces": "^3.0.5",
    "@jupyterlab/services": "^6.0.5",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/disposable": "^1.4.3",
    "@lumino/widgets": "^1.16.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyter_bokeh/labextension"
  },
  "styleModule": "style/index.js"
}